{
  "name": "hashtype-detector",
  "version": "0.0.1",
  "description": "Hashtype detector",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "publish": "npm publish --access public",
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "dev": "microbundle --watch --tsconfig tsconfig.json --no-sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xbenyx/hashtype-detector.git"
  },
  "keywords": [
    "hash",
    "hashcat",
    "hashtype",
    "hashtopolis"
  ],
  "author": "xbenyx",
  "license": "GNUPLv3",
  "bugs": {
    "url": "https://github.com/xbenyx/hashtype-detector/issues"
  },
  "homepage": "https://github.com/xbenyx/hashtype-detector#readme",
  "devDependencies": {
    "typescript": "^5.0.4"
  },
  "files": [
    "lib"
  ]
}